{
"action_class": "dlopt.tools.optimization.Optimizer",
"algorithm_class": "dlopt.architecture.contrib.SelfAdjMuPLambdaUniform",
"data_loader_class": "publications.cities2018.rubbish.RubbishDataLoader",
"data_loader_params": {
"filename": "../../data/waste/rubbish-2013.csv",
"x_features": ["C-A100", "C-A107", "C-A108"],
"y_features": ["C-A100", "C-A107", "C-A108"],
"batch_size" : 5,
"training_ratio": 0.8,
"validation_ratio": 0.2
},
"problem_class": "dlopt.architecture.contrib.TimeSeriesTrainProblem",
"targets": {"mae": -1},
"population_size": 1,
"offspring_size": 1,
"max_eval": 3,
"max_restart": 0,
"migration_population_size": 0,
"p_mutation_i": 0.2,
"p_mutation_e": 0.2,
"mutation_max_step": 15,
"min_neurons": 100,
"max_neurons": 200,
"min_layers": 1,
"max_layers": 1,
"min_look_back": 2,
"max_look_back": 30,
"nn_builder_class": "dlopt.nn.RNNBuilder",
"nn_trainer_class": "dlopt.nn.TrainGradientBased",
"dense_activation": "sigmoid",
"train_epochs": 3,
"test_epochs": 20,
"batch_size" : 5,
"output_logger_class": "dlopt.util.JSONOutput",
"output_logger_params": {"filename": "output.json"},
"model_filename": "model.hdf5"
}
