{
"action_class": "dlopt.tools.optimization.Optimizer",
"algorithm_class": "dlopt.architecture.problems.MuPlusLambda",
"data_loader_class": "examples.sin.sin.SinDataLoader",
"data_loader_params": {
    "start": 0,
    "stop": 100,
    "step": 0.1,
    "batch_size" : 5,
    "training_ratio": 0.8,
    "validation_ratio": 0.2,
    "x_features": ["sin"],
    "y_features": ["sin"]},
"problem_class": "dlopt.architecture.problems.TimeSeriesMAERandSampProblem",
"targets": {"log_p": 1},
"population_size": 3,
"offspring_size": 5,
"max_eval": 10,
"max_restart": 2,
"migration_population_size": 2,
"p_mutation_i": 0.1,
"p_mutation_e": 0.1,
"mutation_scale_factor": 3,
"num_samples": 30,
"min_neurons": 1,
"max_neurons": 10,
"min_layers": 1,
"max_layers": 3,
"min_look_back": 2,
"max_look_back": 20,
"truncated_lower": 0.0,
"truncated_upper": 2.0,
"threshold": 0.01,
"nn_builder_class": "dlopt.nn.RNNBuilder",
"dense_activation": "tanh",
"output_logger_class": "dlopt.util.JSONOutput",
"output_logger_params": {"filename": "output.json"},
"model_filename": "model.hdf5"
}
